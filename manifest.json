{
  "manifest_version": 3,
  "name": "Hyperspec - GMOverlay",
  "description": "Nak≈Çadka do weryfikowania informacji z overpass api",
  "version": "1.0",
  "icons": {
    "128": "assets/icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.google.com/maps/*"],
      "js": [
        "core/storage.js",
        "core/csvParser.js",
        "core/coordinates.js",
        "core/maps.js",
        "core/CSVNavigator.js",
        "content.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["assets/icon.png", "assets/*.csv"],
      "matches": ["<all_urls>"]
    }
  ]
}
